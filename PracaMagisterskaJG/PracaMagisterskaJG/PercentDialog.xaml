<Window x:Class="PracaMagisterskaJG.PercentDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracaMagisterskaJG"
        mc:Ignorable="d"
        Title="Wprowadź dane" Height="175" Width="450">
    <Grid>
        <TextBox x:Name="tbPercent" HorizontalAlignment="Left" Height="23" Margin="298,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="White" TextChanged="tbPercent_TextChanged"/>
        <TextBlock HorizontalAlignment="Left" Margin="23,50,0,0" TextWrapping="Wrap" Text="Podaj stosunek wyrażony w procentach wielkości zbioru treningowego do całego zbioru uczącego" VerticalAlignment="Top" Width="270"/>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="318,101,0,0" VerticalAlignment="Top" Width="74" Click="btnOK_Click"/>

    </Grid>
</Window>
